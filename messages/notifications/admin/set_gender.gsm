{
    "code": "notifications.admin.set_gender",
    "whitelist": [
        "master"
    ],
    "components": {
        "TEXT": {
            "text": "Seleziona il gender per <i>{new_sub[infos][name]} {new_sub[infos][surname]}</i>",
            "actions": []
        },
        "KEYBOARD": {
            "buttons": [
                [
                    {
                        "text": "M️",
                        "id": "m",
                        "actions": [
                            [
                                "REQ",
                                "student_databaser",
                                "edit_student_info",
                                {
                                    "user_id": "$new_sub.user_id",
                                    "info": "gender",
                                    "value": "M"
                                }
                            ],
                            [
                                "REQ",
                                "ghislieri_bot",
                                "edit_groups",
                                {
                                    "user_id": "$new_sub.user_id",
                                    "group": "student",
                                    "edit": "add"
                                }
                            ],
                            [
                                "REQ",
                                "ghislieri_bot",
                                "add_notification",
                                {
                                    "users": [
                                        "$new_sub.user_id"
                                    ],
                                    "n_type": "sub_accept",
                                    "msg_code": "welcome.thank_you",
                                    "notify": true
                                }
                            ],
                            [
                                "REQ",
                                "ghislieri_bot",
                                "expire_notification",
                                {
                                    "user_id": "$new_sub.user_id"
                                }
                            ],
                            [
                                "HOME"
                            ]
                        ]
                    }
                ],
                [
                    {
                        "text": "F️",
                        "id": "f",
                        "actions": [
                            [
                                "REQ",
                                "student_databaser",
                                "edit_student_info",
                                {
                                    "user_id": "$new_sub.user_id",
                                    "info": "gender",
                                    "value": "F"
                                }
                            ],
                            [
                                "REQ",
                                "ghislieri_bot",
                                "edit_groups",
                                {
                                    "user_id": "$new_sub.user_id",
                                    "group": "student",
                                    "edit": "add"
                                }
                            ],
                            [
                                "REQ",
                                "ghislieri_bot",
                                "add_notification",
                                {
                                    "users": [
                                        "$new_sub.user_id"
                                    ],
                                    "n_type": "sub_accept",
                                    "msg_code": "welcome.thank_you",
                                    "notify": true
                                }
                            ],
                            [
                                "REQ",
                                "ghislieri_bot",
                                "expire_notification",
                                {
                                    "user_id": "$new_sub.user_id"
                                }
                            ],
                            [
                                "HOME"
                            ]
                        ]
                    }

                ]
            ],
            "navigation": [
                [
                    [
                        "back"
                    ]
                ]
            ]
        }
    }
}